{
  "version": 2,
  "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ],
  "headers": [
    {
      "source": "/api/GetNewsForNZ",
      "destination": "/api/GetNewsForNZ",
      "headers": [
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "maxage=60"
        },
        {
          "key": "Cache-Control",
          "value": "maxage=60"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "maxage=60"
        }
      ]
    }
  ]
}
